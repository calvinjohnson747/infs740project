<%- include('layout/header3') %>

<div class="container">
    <div class="row my-4">
        <div class="col-lg-12">
            <div class="table-responsive">
                <% if (complex != ''){ %>
                    <table class="table table-striped text-center">
                        <thead>
                            <tr class="table-info">
                                <th>Doctor ID</th>
                                <th>Patient ID</th>
                                <th>Scheduled Day</th>
                                <th>Age</th>
                                <th>Gender</th>
                                <th>Diabetes</th>
                                <th>copd</th>
                                <th>asthma</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% complex.forEach((row, index)=>{ %>
                                <tr>
                                    <td><%= row.Doctor_ID%> </td>
                                    <td><%= row.Result2.Patient_ID%> </td>
                                    <td><%= row.ScheduledDay%> </td>
                                    <td><%= row.Result2.Age%> </td>
                                    <td><%= row.Result2.Gender%> </td>
                                    <td><%= row.Result2.diabetes%> </td>
                                    <td><%= row.Result2.copd%> </td>
                                    <td><%= row.Result2.asthma%> </td>

                                </tr>
                           <% }) %>
                        </tbody>
                    </table>
                <% } else { %>
                    <h1 class="text-center text-secondary mt-5">No appointments found </h1>
                <% } %>
            </div>
        </div>
    </div>
</div>

<%- include('layout/footer3') %>